{
    "name": "lox",
    "publisher": "madeso",
    "displayName": "Lox language",
    "description": "Provides syntax highlighting for the lox language",
    "version": "0.0.1",
    "license": "MIT",
    "engines": {
        "vscode": "^1.30.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/madeso/lox/"
    },
    "categories": ["Programming Languages"],
    "contributes": {
        "languages": [{
            "id": "lox",
            "aliases": ["Lox", "lox", "LOX"],
            "extensions": [".lox"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lox",
            "scopeName": "source.lox",
            "path": "./syntaxes/lox.tmLanguage.json"
        }]
    }
}
